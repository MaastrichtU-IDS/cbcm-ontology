        <owl:versionIRI rdf:resource="http://w3id.org/um/cbcm/eu-cm-ontology/1.2.1"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This ontology describes terms concerning companies, their cross-border movements within the European Union (EU), and associated EU company legislation.</dc:description>
        <dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The European Union Company Mobility Ontology (EU-CMO)</dc:title>
        <terms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Kody Moodley</terms:contributor>
        <terms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Marcus Meyer</terms:contributor>
        <terms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ruta Liepina</terms:contributor>
        <terms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stephan Rammeloo</terms:contributor>
        <terms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Thomas Biermeyer</terms:contributor>
        <terms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tobias Breuer</terms:contributor>
        <terms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Walter Simoncini</terms:contributor>
        <terms:license rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Creative Commons Attribution 3.0 (CC BY 3.0)</terms:license>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://maastrichtu-ids.github.io/cbcm-ontology</rdfs:seeAlso>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.2.1</owl:versionInfo>
        <prov:wasAttributedTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Institute of Data Science at Maastricht University</prov:wasAttributedTo>
        <prov:wasAttributedTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Maastricht University</prov:wasAttributedTo>
        <prov:wasAttributedTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Maastricht Law &amp; Techlab</prov:wasAttributedTo>
    </owl:Ontology>
    


