        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#DivisionPlan"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#EmergingCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#OriginalCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Article 160o: a pre-division certificate attests compliance with all the relevant conditions and the proper completion of all procedures and formalities in the Member State concerned.</rdfs:comment>
        <rdfs:label xml:lang="en">Company pre-division certificate</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.europarl.europa.eu/doceo/document/TA-8-2019-0429_EN.html"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#PreMergerCertificate -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#PreMergerCertificate">
