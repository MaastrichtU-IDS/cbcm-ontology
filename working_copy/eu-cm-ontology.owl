    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#DivisionPlan">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#TransactionDocument"/>
        <rdfs:comment xml:lang="en">The management or administrative organ of the company shall draw up the draft terms of a division. The draft terms of a division shall include at least the particulars listed in article 160d.</rdfs:comment>
        <rdfs:label xml:lang="en">Company division plan</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.europarl.europa.eu/doceo/document/TA-8-2019-0429_EN.html"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#DomesticCompanyConversion -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#DomesticCompanyConversion">
