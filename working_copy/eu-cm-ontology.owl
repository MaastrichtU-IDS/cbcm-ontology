            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An individual, group, or organization that owns one or more shares in a company, and in whose name the share certificate is issued.</rdfs:comment>
        <rdfs:label xml:lang="en">Company shareholder</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ShareholderExitRight -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ShareholderExitRight">
