        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompanyDivision"/>
        <rdfs:comment xml:lang="en">A company which in a process of the domestic division - in case of a full division - transfers all its assets and liabilities to two or more companies, or - in case of a partial division or division by separation - transfers part of its assets and liabilities to one or more companies.</rdfs:comment>
        <rdfs:label xml:lang="en">Company domestic division</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#DomesticMerger -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#DomesticMerger">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompanyMerger"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#AcquiringCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergingCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A domestic merger involves at least two companies based in the same EU Member State and is leading to a universal transfer of assets and liabilities from the merging company to the acquiring company.</rdfs:comment>
        <rdfs:label xml:lang="en">Company domestic merger</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#DomesticSeatTransfer -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#DomesticSeatTransfer">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#SeatTransfer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#TransferOfCentralAdministration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#TransferOfRegisteredOffice"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The act of changing the location where the company’s business was managed (registered office or headquarters) or the place where that company’s primary business was actually conducted (central administration), within a specific country.</rdfs:comment>
        <rdfs:label xml:lang="en">Domestic company seat transfer</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#EmergingCompany -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#EmergingCompany">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Company"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompanyDivision"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#EmergingCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#OriginalCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A company being divided transfers part of its assets and liabilities to one or more newly formed companies, which are the recipient companies in a (cross-border) division.</rdfs:comment>
        <rdfs:label xml:lang="en">Emerging company</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Employee -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Employee">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Stakeholder"/>
        <rdfs:comment xml:lang="en">The workforce of a company. An employee is a party to an employment relationship characterised as a contract of employment (or contract of service) between the employer and employee. However, this is only one of several different legal formulations of the concept of ‘worker’ in the EU which differ between Member States.</rdfs:comment>
        <rdfs:label xml:lang="en">Company employee</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.eurofound.europa.eu/observatories/eurwork/industrial-relations-dictionary/employee"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#FinancialServiceAuthority -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#FinancialServiceAuthority">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompetentAuthority"/>
        <rdfs:comment xml:lang="en">A body that regulates the financial services industry in a country (e.g. the UK), including most financial services markets, exchanges and firms.</rdfs:comment>
        <rdfs:label xml:lang="en">Financial service authority</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#GlobalUltimateOwner -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#GlobalUltimateOwner">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Company"/>
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#UltimateBeneficialOwner"/>
        <rdfs:comment xml:lang="en">A global ultimate owner (GUO) is the individual or entity at the top of the corporate ownership structure.</rdfs:comment>
        <rdfs:label xml:lang="en">Global ultimate owner</rdfs:label>
        <rdfs:seeAlso xml:lang="en">Legal term : Ultimate Beneficial Owner</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ImpactOnEmployment -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ImpactOnEmployment">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#InformationRight"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Employee"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#WorksCouncil"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The right of employees or their representatives to be informed on the likely repercussions of the cross-border transaction on employment, which includes the dismissal as well as the hiring of employees or relocation of work. See for instance article 122 of Directive 2017/1132.</rdfs:comment>
        <rdfs:label xml:lang="en">Impact on employment information right</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://data.europa.eu/eli/dir/2017/1132/oj"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#IndependentExpert -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#IndependentExpert">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/org#Organisation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#IndependentExpertReport"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An Independent Expert provides impartial advice to shareholders and other company stakeholders on the fairness and/or reasonableness of a transaction.  A company may be required to commission an Independent Expert Report (IER) in the case of a (cross-border) merger, division or conversion.</rdfs:comment>
        <rdfs:label xml:lang="en">Independent expert</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://data.europa.eu/eli/dir/2017/1132/oj"/>
        <rdfs:seeAlso rdf:resource="https://www.europarl.europa.eu/doceo/document/TA-8-2019-0429_EN.html"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#IndependentExpertReport -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#IndependentExpertReport">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#TransactionDocument"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#IndependentExpert"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Article 86g and 160i: Member States must ensure that an independent expert examines the draft terms of the transaction and draws up a report intended for members which is made available to them not less than one month before the date of the general meeting. Depending on the law of Member States, the expert may be a natural person or a legal person. The report must assess the cash compensation proposed and valuation method.</rdfs:comment>
        <rdfs:label xml:lang="en">Report by an independent expert</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.europarl.europa.eu/doceo/document/TA-8-2019-0429_EN.html"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#InformationRight -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#InformationRight">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#SocialRight"/>
        <rdfs:comment xml:lang="en">The right of employees or their representatives to be informed and to receive any documents necessary for the exercise of their consultation and board-level employee representation rights in the context of an intended merger/division/conversion/seat transfer.</rdfs:comment>
        <rdfs:label xml:lang="en">Information right</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://data.europa.eu/eli/dir/2002/14/oj"/>
        <rdfs:seeAlso rdf:resource="http://data.europa.eu/eli/dir/2017/1132/oj"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#InterimAccount -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#InterimAccount">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#AnnualAccount"/>
        <rdfs:comment xml:lang="en">Interim accounts are accounts that are published during a company&apos;s financial year and that show the financial situation on a given accounting date.</rdfs:comment>
        <rdfs:label xml:lang="en">Interim account</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#KODYTESTCLASS -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#KODYTESTCLASS"/>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagementBoard -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagementBoard">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Manager"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#partOf"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Company"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The group of persons comprising the governing body of the legal entity that is entrusted with the decision-making in the company. It is also called Executive Board in a one-tier board system.</rdfs:comment>
        <rdfs:label xml:lang="en">Management board</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagementReport -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagementReport">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#TransactionDocument"/>
        <rdfs:comment xml:lang="en">A report drawn up by the company&apos;s management on the intended transaction, laying out the main features of the transactions and its consequences.</rdfs:comment>
        <rdfs:label xml:lang="en">Management report</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Manager -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Manager">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Stakeholder"/>
        <rdfs:comment xml:lang="en">A person or group of persons that are responsible for overseeing the operations of a company.</rdfs:comment>
        <rdfs:label xml:lang="en">Manager</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagerDecisionRight -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagerDecisionRight">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagerRight"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#BoardResolution"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagementBoard"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The right of the board of management or individual managers to take decisions or to not take a decision in the context of a transaction. These are in the form of a board resolution.</rdfs:comment>
        <rdfs:label xml:lang="en">Manager decision right</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagerExitRight -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagerExitRight">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagerRight"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagementBoard"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The right of a manager or the board of management to resign and therefore exit a company.</rdfs:comment>
        <rdfs:label xml:lang="en">Manager exit right</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagerInformationRight -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagerInformationRight">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagerRight"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagementBoard"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The right of a manager to information and access to documents that he or she has in accordance with national or European company law.</rdfs:comment>
        <rdfs:label xml:lang="en">Manager information right</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagerRight -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#ManagerRight">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#TransactionRight"/>
        <rdfs:comment xml:lang="en">The rights of members of the management in the context of a company transaction.</rdfs:comment>
        <rdfs:label xml:lang="en">Manager right</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergerByAcquisition -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergerByAcquisition">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompanyMerger"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#AcquiringCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergingCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergerIntoNewCompany"/>
        <rdfs:comment xml:lang="en">Article 89: The operation whereby one or more companies are wound up without going into liquidation and transfer to another all their assets and liabilities in exchange for the issue to the shareholders of the company or companies being acquired of shares in the acquiring company and a cash payment, if any, not exceeding 10 % of the nominal value of the shares so issued or, where they have no nominal value, of their accounting par value.</rdfs:comment>
        <rdfs:label xml:lang="en">Company merger by acquisition</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://data.europa.eu/eli/dir/2017/1132/oj"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergerCertificate -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergerCertificate">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#TransactionCertificate"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#AcquiringCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompanyMerger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergingCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A document issued by a Competent Authority declaring that the legally required steps of a merger are completed. The certificate needs to be presented by a company to either be registered in another company registry or to be removed from a company register (depending on the company being the merging or acquiring company).</rdfs:comment>
        <rdfs:label xml:lang="en">Company merger certificate</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://data.europa.eu/eli/dir/2017/1132/oj"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergerIntoNewCompany -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergerIntoNewCompany">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompanyMerger"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#EmergingCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergingCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Article 90: the operation whereby several companies are wound up without going into liquidation and transfer to a company that they set up all their assets and liabilities in exchange for the issue to their shareholders of shares in the new company and a cash payment, if any, not exceeding 10 % of the nominal value of the shares so issued or, where they have no nominal value, of their accounting par value.</rdfs:comment>
        <rdfs:label xml:lang="en">Merger into a new company</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://data.europa.eu/eli/dir/2017/1132/oj"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergerPlan -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergerPlan">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#TransactionDocument"/>
        <rdfs:comment xml:lang="en">Article 91: The administrative or management bodies of the merging companies shall draw up draft terms of merger in writing by specifying at least the elements mentioned in article 91.</rdfs:comment>
        <rdfs:label xml:lang="en">Company merger plan</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://data.europa.eu/eli/dir/2017/1132/oj"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergingCompany -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergingCompany">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Company"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#AcquiringCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompanyMerger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Merging Company means the dissolving company that is being acquired by another (acquiring) company in the process of a transaction. In the context of company mobility, this means a universal transfer of assets and liabilities from the merging to the acquiring company (see article 131 of Directive 2017/1132).</rdfs:comment>
        <rdfs:label xml:lang="en">Merging company</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://data.europa.eu/eli/dir/2017/1132/oj"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#NationalRegistry -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#NationalRegistry">
