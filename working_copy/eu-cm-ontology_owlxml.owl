    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#AcquiringCompany</IRI>
        <Literal xml:lang="en">Acquiring Company means the resulting or surviving company in a transaction that takes over another, merging company.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#AcquiringCompany</IRI>
        <Literal xml:lang="en">Acquiring company</Literal>
