    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#AcquiringCompany -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#AcquiringCompany">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Company"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                        <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompanyMerger"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                        <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#MergingCompany"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">Acquiring Company means the resulting or surviving company in a transaction that takes over another, merging company.</rdfs:comment>
        <rdfs:label xml:lang="en">Acquiring company</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://data.europa.eu/eli/dir/2017/1132/oj"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#AnnualAccount -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#AnnualAccount">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#AnnualAccount"/>
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompanyMobilityDocument"/>
        <rdfs:comment xml:lang="en">Annual reports measure a company&apos;s financial health. They focus on past and present financial performance, and make predictions about future prospects.</rdfs:comment>
        <rdfs:comment xml:lang="en">Revenue and financial performance of a company</rdfs:comment>
        <rdfs:label xml:lang="en">Annual account</rdfs:label>
        <rdfs:label xml:lang="en">Annual report</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#BoardLevelEmployeeRepresentationRight -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#BoardLevelEmployeeRepresentationRight">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#SocialRight"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Employee"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#involves"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#WorksCouncil"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Board-level representation of employees (BLER) refers to the phenomenon where employees or their representatives elect or appoint representatives to the strategic decision-making body of a company (i.e. the supervisory board or non-executive board in a one-tier system). If such a system of BLER exists prior to a cross-border company transaction, such right must be upheld in the acquiring/recipient company under the before-and-after-rinciple.</rdfs:comment>
        <rdfs:label xml:lang="en">Board-level employee representation right</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://data.europa.eu/eli/dir/2017/1132/oj"/>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#BoardResolution -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#BoardResolution">
        <rdfs:subClassOf rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Resolution"/>
        <rdfs:comment xml:lang="en">A Board Resolution, or Company Resolution, is a way of documenting a decision made by a Company&apos;s Board of Directors on behalf of the company. After the resolution is made during a meeting, a Board Resolution document is typically used to get the specifics of the resolution in writing.</rdfs:comment>
        <rdfs:label xml:lang="en">Board resolution</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Company -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Company">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/org#Organisation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#hasParentCompany"/>
                <owl:someValuesFrom rdf:resource="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#Company"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A company is a legal entity formed by a group of individuals to engage in and operate a business—commercial or industrial—enterprise. A company may be organized in various ways for tax and financial liability purposes depending on the corporate law of its jurisdiction.</rdfs:comment>
        <rdfs:label xml:lang="en">Company</rdfs:label>
    </owl:Class>
    


    <!-- http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompanyConversion -->

    <owl:Class rdf:about="http://ids.maastrichtuniversity.nl/ontologies/cbm/eu-cm-ontology#CompanyConversion">
